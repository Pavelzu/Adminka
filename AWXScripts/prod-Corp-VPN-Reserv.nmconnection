[connection]
id=Corp-VPN-Reserv
uuid=554fe54f-8f3f-4c8e-bab3-3075f58c627e
type=vpn
autoconnect=false

[vpn]
gateway=ip
ipsec-enabled=yes
machine-auth-type=psk
password-flags=1
user=anyuser
user-auth-type=password
service-type=org.freedesktop.NetworkManager.l2tp

[vpn-secrets]
ipsec-psk=ipseckey

[ipv4]
dns=172.17.0.1;
dns-search=corp.domen.ru;
ignore-auto-routes=true
method=auto
never-default=true
route1=172.16.0.0/12,10.30.0.1

[ipv6]
addr-gen-mode=stable-privacy
method=auto

[proxy]
